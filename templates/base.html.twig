<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('assets/css/reset.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/css/animated-logo.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/css/our-team.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/css/custom.css') }}" />


            {# block css link #}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('assets/js/modernizr.js') }}"></script>

            {#block javacascript#}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
    {% block nav %}
        {% include ('theme/header.html.twig') %}

    {% endblock %}
    <div class="body">

    <div class="container">
        {% block body %}



        {% endblock %}
    </div>
    </div>
        {% block footer %}
            {% include ('theme/footer.html.twig') %}
        {% endblock %}
        {# block javascript footer#}
    {% block javascriptfooter %}
        <script src="{{ asset('assets/js/modernizr.js') }}"></script>
        <script src="{{ asset('assets/js/jquery-2.1.1.js') }}"></script>
        <script src="{{ asset('assets/js/masonry.pkgd.min.js') }}"></script>
        <script src="{{ asset('assets/js/jquery.flexslider-min.js') }}"></script>
        <script src="{{ asset('assets/js/main.js') }}"></script>
        <script src="{{ asset('assets/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('assets/js/agency.js') }}"></script>
        <script src="{{ asset('assets/js/jquery.easing.min.js') }}"></script>
        <script src="{{ asset('assets/js/classie.js') }}"></script>
        <script src="{{ asset('assets/js/cbpAnimatedHeader.js') }}"></script>

    {% endblock %}
    </body>
</html>

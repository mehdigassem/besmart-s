<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
       and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/css/sb-admin.min.css') }}"/>
        {#<link rel="stylesheet" href="{{ asset('assets/css/all.css') }}"/>#}
        <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
              integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p"
              crossorigin="anonymous"/>

        <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
              rel="stylesheet">
        {# block css link #}
        {#{{ encore_entry_link_tags('app') }}#}
    {% endblock %}

    {% block javascripts %}

        {#block javacascript#}
        {#{{ encore_entry_script_tags('app') }}#}
    {% endblock %}
</head>
<body id="page-top">
<div id="wrapper">

    {% block dash %}
        {% include ('admin/dash.html.twig') %}

    {% endblock %}
    <div id="content-wrapper" class="d-flex flex-column">
        {% block nav %}
            {% include ('admin/nav.html.twig') %}

        {% endblock %}
        {% block body %}



        {% endblock %}

        {% block footer %}
            {% include ('admin/footer.html.twig') %}

        {% endblock %}
    </div>
</div>
{# block javascript footer#}

{% block javascriptfooter %}
    <script src="{{ asset('assets/js/jquery.min.js') }}"></script>
    <script src="{{ asset('assets/js/bootstrap/js/bootstrap.bundle.min.js') }}"></script>
    <script src="{{ asset('assets/js/jquery.easing.min.js') }}"></script>
    <script src="{{ asset('assets/js/sb-admin-2.min.js') }}"></script>
{% endblock %}

</body>
</html>
